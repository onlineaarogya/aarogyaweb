dy1\"\n                  color=\"textPrimary\"\n                  className={clsx(classes.listItemText, 'menu-item')}\n                >\n                  {page.title}\n                </Typography>\n\n                <ListItemIcon className={classes.listItemIcon}>\n                  <ExpandMoreIcon\n                    className={\n                      openedPopoverId === page.id ? classes.expandOpen : ''\n                    }\n                    fontSize=\"small\"\n                  />\n                </ListItemIcon>\n              </ListItem>\n              <Popover\n                elevation={1}\n                id={page.id}\n                open={openedPopoverId === page.id}\n                anchorEl={anchorEl}\n                onClose={handleClose}\n                anchorOrigin={{\n                  vertical: 'bottom',\n                  horizontal: 'center',\n                }}\n                transformOrigin={{\n                  vertical: 'top',\n                  horizontal: 'center',\n                }}\n                classes={{ paper: classes.popover }}\n              >\n                <div>{renderPages(page.id)}</div>\n              </Popover>\n            </div>\n          ))}\n\n          <Link\n            href=\"/\"\n            color=\"textPrimary\"\n            variant=\"body1\"\n            className={clsx(classes.listItemText, 'menu-item')}\n          >\n            <Typography\n              variant=\"body1\"\n              color=\"textPrimary\"\n              className={clsx(\n                classes.listItemText,\n                'menu-item',\n                classes.pointer,\n                classes.navLink,\n              )}\n            >\n              About\n            </Typography>\n          </Link>\n          <Link\n            href=\"/\"\n            color=\"textPrimary\"\n            variant=\"body1\"\n            className={clsx(classes.listItemText, 'menu-item')}\n          >\n            <Typography\n              varia